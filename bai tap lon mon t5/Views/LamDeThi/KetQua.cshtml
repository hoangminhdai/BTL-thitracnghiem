
@{
    ViewBag.Title = "KetQua";
    Layout = "~/Views/Shared/_LayoutLamDE.cshtml";

}
<h2 style="text-align:center; color:red; font-size:60px;">Kết Quả</h2>
<div style="border:1px solid; background-color:aquamarine;text-align:center;">
    <p style="font-size: x-large;margin-top: 42px;margin-bottom: 1rem;"><span>Mã Đề </span><span>:</span><span>@ViewBag.made</span></p>
    <p style="font-size: x-large;margin-top: 42px;margin-bottom: 1rem;"><span>Số Câu Đúng </span><span>:</span><span>@ViewBag.A</span></p>
    <p style="font-size: x-large; margin-top: 42px;margin-bottom: 1rem;"><span>Điểm Thi </span><span>:</span><span>@ViewBag.diem</span></p>
</div>
<div style="
    text-align: center;
    padding-top: 12px;
">
    <a href="@Url.Action("Index","Home")">
        <input type="button" value="Quay Về" style="border-radius:6px;width:90px;">
    </a>
</div>



